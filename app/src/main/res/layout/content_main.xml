<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="groclist.edu.fsu.cs.mobile.groclist.MainActivity"
    tools:showIn="@layout/app_bar_main">


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/main_frame"
        android:tag="main_frag"
        android:layout_weight="1"
        android:name="edu.fsu.cs.mobile.groclist.mainFragment"
        />
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/current_frame"
        android:tag="current_list"
        android:layout_weight="1"
        android:name="edu.fsu.cs.mobile.groclist.currentFragment"
        />
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/past_frame"
        android:tag="past_list"
        android:layout_weight="1"
        android:name="edu.fsu.cs.mobile.groclist.pastFragment"
        />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/pantry_frame"
        android:tag="pantry_list"
        android:layout_weight="1"
        android:name="edu.fsu.cs.mobile.groclist.pantryFragment"
        />







</RelativeLayout>
